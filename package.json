{
  "scripts": {
    "start": "NODE_ENV=development run-p dev:*",
    "dev:frontend": "node ./scripts/esbuild.dev.js",
    "dev:backend": "node ./server.js",
    "serve": "run-p serve:*",
    "serve:build": "node ./scripts/esbuild.prod.js",
    "serve:start": "node ./server.js",
    "lint": "run-s lint:format lint:fix",
    "lint:fix": "eslint \"{frontend,stories}/**/*.{js,jsx}\" --fix",
    "lint:format": "prettier -u --write frontend/**/*.{js,jsx}",
    "openapi": "run-s openapi:generate openapi:bundle",
    "openapi:generate": "bash scripts/generate.sh service zerotier-one-api",
    "openapi:bundle": "tsc zerotier-one-api/index.ts"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.2.1",
    "@chakra-ui/storybook-addon": "^4.0.1",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fastify/http-proxy": "^8.2.1",
    "@fastify/static": "^6.4.0",
    "@tanstack/react-query": "^4.0.10",
    "@tanstack/react-query-devtools": "^4.0.10",
    "axios": "^0.27.2",
    "fastify": "^4.1.0",
    "framer-motion": "^6.3.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "url-parse": "^1.5.10",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "babel-loader": "^8.2.5",
    "esbuild": "^0.14.47",
    "eslint": "^8.18.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "install": "^0.13.0",
    "npm": "^8.13.1",
    "npm-run-all": "^4.1.5"
  }
}
